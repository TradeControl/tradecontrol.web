@page
@model TradeControl.Web.Pages.Cash.Transfer.EditModel
@{
    ViewData["Title"] = "Edit Asset Entry";
}
<h1>@ViewData["Title"]</h1>
<h3>@Html.DisplayFor(model => model.Cash_TransfersUnposted.PaymentCode)</h3>
<hr />

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Cash_TransfersUnposted.AccountCode)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Cash_TransfersUnposted.AccountCode)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Cash_TransfersUnposted.SubjectCode)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Cash_TransfersUnposted.SubjectCode)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Cash_TransfersUnposted.CashCode)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Cash_TransfersUnposted.CashCode)
    </dd>
</dl>

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <input type="hidden" asp-for="Cash_TransfersUnposted.UserId" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.PaymentCode" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.SubjectCode" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.AccountCode" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.PaymentStatusCode" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.TaxCode" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.CashCode" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.InsertedBy" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.InsertedOn" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.UpdatedBy" />
            <input type="hidden" asp-for="Cash_TransfersUnposted.UpdatedOn" />

            <div class="mb-3">
                <label asp-for="Cash_TransfersUnposted.PaidOn" class="control-label"></label>
                <input asp-for="Cash_TransfersUnposted.PaidOn" class="form-control" />
                <span asp-validation-for="Cash_TransfersUnposted.PaidOn" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Cash_TransfersUnposted.PaidInValue" class="control-label"></label>
                <input asp-for="Cash_TransfersUnposted.PaidInValue" class="form-control" />
                <span asp-validation-for="Cash_TransfersUnposted.PaidInValue" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Cash_TransfersUnposted.PaidOutValue" class="control-label"></label>
                <input asp-for="Cash_TransfersUnposted.PaidOutValue" class="form-control" />
                <span asp-validation-for="Cash_TransfersUnposted.PaidOutValue" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Cash_TransfersUnposted.PaymentReference" class="control-label"></label>
                <input asp-for="Cash_TransfersUnposted.PaymentReference" class="form-control" />
                <span asp-validation-for="Cash_TransfersUnposted.PaymentReference" class="text-danger"></span>
            </div>

            <div class="mb-3">
                <input type="submit" value="SAVE" class="btn btn-primary" />
                <a asp-page="./Index" asp-route-cashSubjectCode="@Model.Cash_TransfersUnposted.AccountCode" class="btn btn-info">To List</a>
            </div>
        </form>
    </div>
</div>
