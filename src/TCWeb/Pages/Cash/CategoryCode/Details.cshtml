@page
@model TradeControl.Web.Pages.Cash.CategoryCode.DetailsModel
@{ ViewData["Title"] = "Category Details"; }
<h1>@ViewData["Title"]</h1>

<div>
    <h4>@Html.DisplayFor(model => model.Cash_Category.Category)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Category.CategoryCode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Category.CategoryCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Category.CategoryType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Category.CategoryType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Category.CashType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Category.CashType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Category.CashPolarity)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Category.CashPolarity)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Category.DisplayOrder)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Category.DisplayOrder)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Category.IsEnabled)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Category.IsEnabled)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Index" asp-route-cashTypeCode="@Model.Cash_Category.CashTypeCode" class="btn btn-info">To List</a>
</div>
