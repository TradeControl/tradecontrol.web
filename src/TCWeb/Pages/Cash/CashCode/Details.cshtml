@page
@model TradeControl.Web.Pages.Cash.CashCode.DetailsModel
@{ ViewData["Title"] = "Cash Code Details"; }
<h1>@ViewData["Title"]</h1>

<div>
    <h4>@Html.DisplayFor(model => model.Cash_Code.CashDescription)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.CashCode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.CashCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.Category)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.Category)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.CashMode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.CashMode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.TaxDescription)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.TaxDescription)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.CashType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.CashType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.IsCashEnabled)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.IsCashEnabled)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cash_Code.IsCategoryEnabled)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cash_Code.IsCategoryEnabled)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Index" asp-route-category="@Model.Cash_Code.Category" class="btn btn-info">To List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
