@page
@model TradeControl.Web.Pages.Tax.TaxCode.DeleteModel
@{ ViewData["Title"] = "Tax Code Deletion"; }
<h1>@ViewData["Title"]</h1>

<div>
    <h4>@Html.DisplayFor(model => model.App_TaxCode.TaxDescription)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_TaxCode.TaxCode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_TaxCode.TaxCode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_TaxCode.TaxType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_TaxCode.TaxType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_TaxCode.TaxRate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_TaxCode.TaxRate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_TaxCode.Rounding)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_TaxCode.Rounding)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_TaxCode.Decimals)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_TaxCode.Decimals)
        </dd>
    </dl>
    <form method="post">
        <input type="hidden" asp-for="@Model.App_TaxCode.TaxCode" />
        <input type="submit" value="DELETE" class="btn btn-danger" />
        <a asp-page="./Index" asp-route-taxTypeCode="@Model.App_TaxCode.TaxTypeCode" class="btn btn-info">To List</a>
    </form>
</div>
