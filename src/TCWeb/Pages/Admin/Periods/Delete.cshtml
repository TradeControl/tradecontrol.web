@page
@model TradeControl.Web.Pages.Admin.Periods.DeleteModel
@{ ViewData["Title"] = "Delete Financial Period"; }
<h1>@ViewData["Title"]</h1>

<div>
    <h4>@Html.DisplayFor(model => model.App_Year.Description)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_Year.YearNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_Year.YearNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_Year.StartMonth)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_Year.StartMonth)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_Year.CashStatus)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_Year.CashStatus)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_Year.InsertedBy)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_Year.InsertedBy)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.App_Year.InsertedOn)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.App_Year.InsertedOn)
        </dd>
    </dl>
    <form method="post">
        <input type="hidden" asp-for="@Model.App_Year.YearNumber" />
        <input type="submit" value="DELETE" class="btn btn-danger" />
        <a asp-page="./Index" class="btn btn-info">To List</a>
    </form>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

