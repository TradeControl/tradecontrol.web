@page
@model TradeControl.Web.Pages.Org.Contact.DeleteModel
@{ ViewData["Title"] = "Delete Contact"; }
<h1>@ViewData["Title"]</h1>

<div>
    <h4>@Html.DisplayFor(model => model.Contact.ContactName)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.MobileNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.MobileNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.PhoneNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.PhoneNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.EmailAddress)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.EmailAddress)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.Department)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.Department)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.AccountName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.AccountName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.InsertedBy)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.InsertedBy)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contact.InsertedOn)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contact.InsertedOn)
        </dd>

    </dl>
</div>

<form method="post">
    <input type="hidden" asp-for="Contact.AccountCode" />
    <input type="hidden" asp-for="Contact.ContactName" />
    <input type="submit" value="DELETE" class="btn btn-danger" />
    <a asp-page="./Index" asp-route-accountCode="@Model.Contact.AccountCode" class="btn btn-info">To Listing</a>
</form>

